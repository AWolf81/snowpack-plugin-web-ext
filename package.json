{
  "name": "snowpack-plugin-web-ext",
  "version": "0.1.1",
  "description": "Web ext plugin for snowpack",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AWolf81/snowpack-plugin-web-ext.git"
  },
  "keywords": [
    "snowpack",
    "snowpack-plugin"
  ],
  "scripts": {
    "build": "pika build",
    "deploy": "pika publish --any-branch",
    "version": "npm run build",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "report-coverage": "codecov --file=./coverage/lcov.info"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "web-ext": ">=5.x"
  },
  "devDependencies": {
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@schemastore/package": "0.0.6",
    "@tsconfig/node12": "1.0.7",
    "@types/chai": "4.2.14",
    "@types/chai-spies": "1.0.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "codecov": "3.8.1",
    "cross-env": "7.0.3",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "husky": "4.3.7",
    "jest": "26.6.3",
    "mocha": "8.2.1",
    "node-notifier": "9.0.0",
    "npm-force-resolutions": "0.0.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "sinon": "9.2.3",
    "snowpack": "2.18.5",
    "source-map-support": "0.5.19",
    "ts-jest": "26.4.4",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.9.7",
    "url-regex-safe": "1.0.2",
    "web-ext": "^5.5.0"
  },
  "author": "AWolf81",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
