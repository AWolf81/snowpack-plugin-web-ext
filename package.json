{
  "name": "web-ext-snowpack-plugin",
  "version": "0.0.1",
  "description": "Web ext plugin for snowpack",
  "scripts": {
    "build": "pika build",
    "deploy": "pika publish --any-branch",
    "version": "npm run build"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "web-ext": ">=5.x",
    "json-templates": ">=4.x"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/json-templates": "3.0.0",
    "@types/node": "14.14.20",
    "snowpack": "^2.6.4",
    "typescript": "^3.9.7",
    "web-ext": "^5.4.1",
    "json-templates": "^4.1.0"
  },
  "author": "AWolf81",
  "license": "MIT"
}
